#include <stdio.h>

int main() {
    float r, a, l, w;
    float circle_area, circle_perimeter;
    float square_area, square_perimeter;
    float rect_area, rect_perimeter;
    const float PI = 3.1416;

    // Input
    scanf("%f", &r);          // radius of circle
    scanf("%f", &a);          // side of square
    scanf("%f %f", &l, &w);   // length and width of rectangle

    // Circle calculations
    circle_area = PI * r * r;
    circle_perimeter = 2 * PI * r;

    // Square calculations
    square_area = a * a;
    square_perimeter = 4 * a;

    // Rectangle calculations
    rect_area = l * w;
    rect_perimeter = 2 * (l + w);

    // Output
    printf("Circle: %.2f %.2f\n", circle_area, circle_perimeter);
    printf("Square: %.2f %.2f\n", square_area, square_perimeter);
    printf("Rectangle: %.2f %.2f\n", rect_area, rect_perimeter);

    return 0;
}
