#include <stdio.h>

int main() {
    int physics, chemistry, biology, mathematics, computer;
    float percentage;
    char grade;
    
    // Read marks for the five subjects
    scanf("%d %d %d %d %d", &physics, &chemistry, &biology, &mathematics, &computer);
    
    // Calculate total marks and percentage
    int total = physics + chemistry + biology + mathematics + computer;
    percentage = (total / 500.0) * 100;
    
    // Assign grade based on percentage
    if (percentage >= 90) {
        grade = 'A';
    } else if (percentage >= 80) {
        grade = 'B';
    } else if (percentage >= 70) {
        grade = 'C';
    } else if (percentage >= 60) {
        grade = 'D';
    } else if (percentage >= 40) {
        grade = 'E';
    } else {
        grade = 'F';
    }
    
    // Print percentage with 2 decimal places and the grade
    printf("Percentage = %.2f\n", percentage);
    printf("Grade = %c\n", grade);
    
    return 0;
}
