#include <stdio.h>

int main() {
    int N;
    
    // Read the integer input
    scanf("%d", &N);
    
    // Check the value of N and print accordingly
    if (N > 0) {
        printf("Positive\n");
    } else if (N < 0) {
        printf("Negative\n");
    } else {
        printf("Zero\n");
    }
    
    return 0;
}
