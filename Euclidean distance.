#include <stdio.h>

#include <math.h>

int main() {

    int x1, y1, x2, y2;

    double distance;

    // Input coordinates

    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);

    // Euclidean distance formula

    distance = sqrt((x2 - x1) * (x2 - x1) + 

                    (y2 - y1) * (y2 - y1));

    // Print result with 2 decimal places

    printf("%.2lf\n", distance);

    return 0;

}
